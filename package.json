{
  "name": "@qavajs/template",
  "version": "0.8.2",
  "description": "library that allow to define step definitions on Gherkin language",
  "scripts": {
    "build": "tsc",
    "test": "vitest --coverage run",
    "test:e2e": "qavajs run --config test-e2e/template.ts",
    "test:e2e:debug": "qavajs run --config test-e2e/template.ts --tags @debug",
    "lint:write": "npm run pretty && eslint ./src/ --fix",
    "pretty": "prettier --write ./src/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qavajs/template.git"
  },
  "authors": [
    "Alexandr Galichenko",
    "Alexandr Legchilov"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/qavajs/template/issues"
  },
  "homepage": "https://github.com/qavajs/template#readme",
  "devDependencies": {
    "@cucumber/cucumber": "^10.3.2",
    "@cucumber/gherkin-streams": "^5.0.1",
    "@cucumber/messages": "^24.1.0",
    "@qavajs/cli": "^0.37.0",
    "@qavajs/console-formatter": "^0.7.2",
    "@qavajs/memory": "^1.7.0",
    "@types/glob": "^8.1.0",
    "@types/node": "^20.12.5",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "@vitest/coverage-v8": "^1.4.0",
    "husky": "^9.0.11",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.4",
    "vitest": "^1.4.0"
  },
  "dependencies": {
    "glob": "^10.3.12"
  }
}
